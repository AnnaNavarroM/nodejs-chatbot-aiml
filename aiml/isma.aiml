<?xml version="1.0" encoding="ISO-8859-1"?>
<aiml>
  <category>
    <pattern>SIAPA * KAMU</pattern>
    <template>Nama ku <bot name="name"/>.</template>
  </category>


  <category>
    <pattern>NAMA KAMU SIAPA</pattern>
    <template><srai>SIAPA * KAMU</srai></template>
  </category>


  <category>
    <pattern>NAMA SAYA * <set name="clientName">*</set></pattern>
    <template>
      <random>
        <li>Hi <get name="clientName"/>!</li>
        <li>Hey <get name="clientName"/>!</li>
      </random>
    </template>
  </category>

  <category>
    <pattern>SIAPA SAYA</pattern>
    <template>
      <random>
      <li>Namamu kan <get name="clientName"/></li>
      <li><get name="clientName"/>! Masa lupa?</li>
      <li><get name="clientName"/>.</li>
      </random>
    </template>
  </category>

  <category>
    <pattern>KAMU SIAPA</pattern>
    <template>
      <srai>SIAPA * KAMU</srai>
    </template>
  </category>

  <category>
    <pattern>KASIH * HURUF</pattern>
    <template>
      <random>
        <li>A</li>
        <li>B</li>
        <li>C</li>
      </random>
    </template>
  </category>

  <category>
    <pattern>TEST SR TAG</pattern>
    <template>
      <sr/>
    </template>
  </category>

  <category>
    <pattern>TEST SR IN RANDOM</pattern>
    <template>
      <random>
        <li>
          <sr/>
        </li>
        <li>
          <srai>WHAT IS YOUR NAME</srai>
        </li>
      </random>
    </template>
  </category>
  <category>
    <pattern>TEST THE STAR TAG</pattern>
    <template>
      <star/>
    </template>
  </category>
  <category>
    <pattern>TEST THE THAT TAG</pattern>
    <template>I start testing that.</template>
  </category>
  <category>
    <pattern>TEST THAT-TAG. MATCH</pattern>
    <that>I start testing that.</that>
    <template>That matched quite well!</template>
  </category>
  <category>
    <pattern>TEST THAT-TAG. DONT MATCH</pattern>
    <that>I found something.</that>
    <template>Sorry, that did not match!</template>
  </category>
  <category>
    <pattern>KAMU MERASA <set name="state">*</set></pattern>
    <template>Yeah! Saya merasa <get name="state"/>!</template>
  </category>

  <category>
    <pattern>BAGAIMANA PERASAANMU</pattern>
    <template>
      <condition name="state" value="senang">senang xD</condition>
      <condition name="state" value="sedih">sedih.... </condition>
    </template>
  </category>

  <category>
    <pattern>* PERASAAN * </pattern>
    <template>
      <condition>
        <li name="state" value="senang">Wonderful!</li>
        <li name="state" value="sedih">:(</li>
        <li>Hari ini biasa aja</li>
      </condition>
    </template>
  </category>

  <category>
    <pattern>BAGAIMANA PERASAANMU</pattern>
    <template>
      <condition name="state">
        <li value="happy">Senang! </li>
        <li value="sad">aga sedih nih hari ini </li>
        <li>Ga kenapa-kenapa</li>
      </condition>
    </template>
  </category>

  <category>
    <pattern>JELASKAN *</pattern>
    <template>Apa ya <star/> itu?</template>
  </category>

  <category>
    <pattern>Umur Saya *</pattern>
    <template>Text before random<random><li><think><set name="clientAge"><star/></set></think></li><li><think><set name="clientAge">22</set></think></li></random> Text after random</template>
  </category>
  <category>
    <pattern>Berapa * Saya</pattern>
    <template>Kamu <get name="clientAge"/> tahun.</template>
  </category>
  <category>
    <pattern>WHAT DO YOU KNOW ABOUT ME</pattern>
    <template>Your name is <get name="clientName"/> and you are <get name="clientAge"/></template>
  </category>
  <category>
    <pattern>TEST CONDITION AND SRAI</pattern>
    <template>
      <condition name="state">
        <li value="happy">Feeling happy! <srai>WHAT IS YOUR NAME</srai></li>
        <li value="sad">Feeling sad today <srai>WHAT IS MY NAME</srai></li>
        <li>I don't feel anything <srai>HOW OLD AM I</srai></li>
      </condition>
    </template>
  </category>
  <category>
    <pattern>DO UPPERCASE <set name="clientName">*</set></pattern>
    <template>Hello <uppercase><get name="clientName" /></uppercase></template>
  </category>
  <category>
    <pattern>DO LOWERCASE <set name="clientName">*</set></pattern>
    <template>Hello <lowercase><get name="clientName" /></lowercase></template>
  </category>
  <category>
    <pattern>DO FORMAL CASE <set name="clientName">*</set></pattern>
    <template>Hello <formal><get name="clientName" /></formal></template>
  </category>
  <category>
    <pattern>DO SENTENCE <set name="clientName">*</set></pattern>
    <template><sentence><get name="clientName" /></sentence> is the best.</template>
  </category>
  <category>
    <pattern>SUPPORT BR TAGS</pattern>
    <template>This<br/>is<br/>a<br/>bot.</template>
  </category>
  <category>
    <pattern>I LIKE GIFS</pattern>
    <template>GIF this: <img src="GIF" /></template>
  </category>


  <category>
    <pattern>KAPAN NIKAH</pattern>
    <template>
      <random>
        <li>kapan ya?</li>
        <li>gatau tuh!</li>
      </random>
    </template>
  </category>


